{
  "version": 2,
  "builds": [
      {
          "src": "server/index.js",
          "use": "@vercel/python"
      }
  ],
  "rewrites": [
      { "source": "/(.*)", "destination": "server/index.js" }
  ],
  "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      },
      {
        "source": "https://currency-converter-qaze.vercel.app/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }  
  ]
}